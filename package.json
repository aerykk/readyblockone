  "scripts": {
    "start": "./node_modules/.bin/babel-node index.js",
    "start-prod": "NODE_ENV=production ./node_modules/.bin/babel-node index.js",
    "start-web-server": "./node_modules/.bin/babel-node ./Services/WebService/index.js",
    "start-data-server": "./node_modules/.bin/babel-node ./Services/DataService/index.js",
    "lint": "./node_modules/.bin/eslint -c .eslintrc Framework Services",
    "pack": "npm prune && npm dedupe && npm shrinkwrap --dev && shrinkpack",
    "webpack": "./node_modules/.bin/babel-node ./node_modules/.bin/webpack --progress --colors",
    "build": "./node_modules/.bin/webpack --display-chunks --optimize-min-chunk-size 10000 --optimize-max-chunks 10 --display-reasons --progress --color",
    "push": "git push",
    "push-rebuild": "NODE_ENV=production ./node_modules/.bin/webpack --progress -p && git add -A && git commit -m \"Rebuid\" && git push",
    "styleguide-server": "./node_modules/.bin/styleguidist server",
    "styleguide-build": "./node_modules/.bin/styleguidist build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stokegames/horadric.git"
  },
